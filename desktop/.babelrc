{
  "comments": false,
  "env": {
    "testing-unit": {
      "presets": ["es2015", "stage-0"],
      "plugins": ["istanbul"]
    },
    "testing-e2e": {
      "presets": ["es2015", "stage-0"]
    },
    "main": {
      "presets": ["es2015", "stage-0"]
    },
    "renderer": {
      "presets": [
        ["es2015"],
        "stage-0"
      ],
      "plugins":[
        "syntax-flow",
        "transform-flow-strip-types"
      ]
    }
  },
  "plugins": ["transform-runtime"]
}
